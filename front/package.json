    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.3",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.22",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-html": "^2.1.1",
    "vue-tsc": "^0.3.0"
  }
